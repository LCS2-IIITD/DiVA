        function openTab(evt, cityName, group) {
            var i, tabcontent, tablinks;
            tabcontent = document.getElementsByClassName(`tabcontent ${group}`);
            for (i = 0; i < tabcontent.length; i++) {
                tabcontent[i].style.display = "none";
            }
            tablinks = document.getElementsByClassName(`tablinks ${group}`);
            for (i = 0; i < tablinks.length; i++) {
                tablinks[i].className = tablinks[i].className.replace(" active", "");
            }
            document.getElementById(cityName).style.display = "block";
            evt.currentTarget.className += " active";
        }

        document.querySelectorAll("#defaultOpen").forEach((elm) => {
            elm.click();
        })

        var acc = document.getElementsByClassName("accordion");
        var acc_i;

        for (acc_i = 0; acc_i < acc.length; acc_i++) {
            acc[acc_i].addEventListener("click", function() {
                /* Toggle between adding and removing the "accordian-active" class,
                to highlight the button that controls the panel */
                this.classList.toggle("accordian-active");

                /* Toggle between hiding and showing the accordian-active panel */
                var panel = this.nextElementSibling;
                if (panel.style.display === "block") {
                panel.style.display = "none";
                } else {
                panel.style.display = "block";
                }
            });
        }

        function changeOnSelect(groupNum, target) {
