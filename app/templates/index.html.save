    <style>
        /*
            LAYOUT CSS
        */
        * {
            box-sizing: border-box;
            font-family: 'Open Sans', sans-serif !important;
            font-size: 11px !important;
        }
        
        html, body {
            height: 100vh;
            width: 100vw;
        }

        .flex-container-main {
        /* We first create a flex layout context */
            display: flex;
            
            /* Then we define the flow direction 
                and if we allow the items to wrap 
            * Remember this is the same as:
            * flex-direction: row;
            * flex-wrap: wrap;
            */
            flex-flow: nowrap;
            
            /* Then we define how is distributed the remaining space */
            justify-content: space-around;
            
            padding: 0;
            margin: 0;
            list-style: none;
            height: 100%;
        }

        .font-head {
            font-size: 14px !important;
        }

        .font-sub {
            font-size: 11px !important;
        }

        .font-12 {
            font-size: 12px !important;
        }

        .flex-item-100 {
            height: 100%;
        }

        .footer {
            height: 40px;
            width: 100%;
            position: relative;
            bottom: 0;
            background-color: #ccc;
        }

        .graph-container {
            /*min-height: calc(100vh - 40px);*/
            height: 100%;
        }

        .vertical-horizontal-center {
            display: flex;
            justify-content: center;
            align-items: center;
        }

        #myProgress {
            width: 60%;
            background-color: #353A40;
            border-radius: 10px;
        }

        #myBar {
            width: 100%;
            height: 15px;
            background-color: #353A40;
            border-radius: 10px;
        }

        .tab {
            height: 25px;
            overflow-x: auto;
            border-bottom: 1px solid rgba(0,0,0,.125);
            background-color: rgba(0,0,0,.03);
            white-space: nowrap;
            display: flex;
            flex-wrap: nowrap;
            z-index: 1000;
        }

        /* Style the buttons inside the tab */
        .tab button {
            background-color: inherit;
            display: inline-block;
            float: left;
            border: none;
            outline: none;
            cursor: pointer;
            padding: 5px 5px;
            transition: 0.3s;
            font-size: 18px;   
        }

        /* Change background color of buttons on hover */
        .tab button:hover {
            background-color: #ddd;
        }

        /* Create an active/current tablink class */
        .tab button.active {
            background-color: #bbb;
        }

        /* Style the tab content */
        .tabcontent {
            display: none;
            border-top: none;
            overflow: hidden;
            z-index: 1;
            height: calc(100% - 25px);
        }

        .tablinks {
            font-size: 13px !important;
        }

        ::-webkit-scrollbar {
        width: 5px;
        height: 5px;
        }

        /* Track */
        ::-webkit-scrollbar-track {
        background: #f1f1f1; 
        }
        
        /* Handle */
        ::-webkit-scrollbar-thumb {
        background: #888; 
        }

        /* Handle on hover */
        ::-webkit-scrollbar-thumb:hover {
        background: #555; 
        }

        .bold-text {
            font-weight: 900;
        }

        /* Style the buttons that are used to open and close the accordion panel */
        .accordion {
            background-color: #eee;
            color: #444;
            cursor: pointer;
            width: 100%;
            text-align: left;
            border: none;
            outline: none;
            transition: 0.4s;
        }

        /* Add a background color to the button if it is clicked on (add the .accordian-active class with JS), and when you move the mouse over it (hover) */
        .accordion:hover {
            background-color: #ccc;
        }

        .accordian-active {
            background-color: #ccc;
        }

        /* Style the accordion panel. Note: hidden by default */
        .panel {
            background-color: white;
            display: none;
            overflow: hidden;
        }

        .accordion:after {
            content: '\02795'; /* Unicode character for "plus" sign (+) */
            font-size: 13px;
            color: #777;
            float: right;
            margin-left: 5px;
        }

        .accordian-active:after {
            content: "\2796"; /* Unicode character for "minus" sign (-) */
        }

        .diff-group {
            display: none;
        }

        .main-holder {
            height: calc(100% - 65px);
            width: 100%;
        }

        #range-wrap {
            position: relative;
        }

        .bubble {
            background: red;
            color: white;
            padding: 4px 12px;
            position: relative;
            border-radius: 4px;
            left: -50%;
            transform: translateX(50%);
        }
        .bubble::after {
            content: "";
            position: absolute;
            width: 2px;
            height: 2px;
            background: red;
            top: -1px;
            left: 50%;
        }

    </style>
</head>
