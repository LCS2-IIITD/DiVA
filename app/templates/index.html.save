        function getDiffusionData(timestamp) {
            var sliderTime = document.getElementById("slider-time");
            sliderTime.innerHTML = timestamp;
            document.getElementById("myBar").value = timestamp
            let allClasses = Object.keys(timelineDataGlobal)
            
            console.log(allClasses)
            let startTime = 0
            let statuses = {}
            for (let j = 0; j < timestamp; j++) {
                for (let i = 0; i < allClasses.length; i++) {
                    timelineDataGlobal[i][j].map(function(d) {
                        statuses[d] = i
                    })
                } 
            }
            console.log(statuses)
            let answer = {}
            for (let i = 0; i < allClasses.length; i++) {
                answer[i] = 0
            }
            
            let statusKeys = Object.keys(statuses)
            console.log(statusKeys)
            for (let i = 0; i < statusKeys.length; i++) {
                answer[statuses[statusKeys[i]]] += 1
            }
            //console.log(answer)
            for (let i = 0; i < allClasses.length; i++) {
                document.getElementById(`class_${i}_stat`).innerHTML = answer[i]
            }
        }
